
<MudMenu Icon="@Icons.Material.Outlined.Language" Color="Color.Inherit" AnchorOrigin="Origin.BottomCenter" TransformOrigin="Origin.TopCenter" Dense="true">
    @foreach (var language in _supportedCultures)
{
    <MudMenuItem OnClick="(()=> ChangeLanguageAsync(language))">@language.DisplayName</MudMenuItem>
}
</MudMenu>